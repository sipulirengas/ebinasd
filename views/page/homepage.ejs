<section class="bg-primary" id="about">
    <div class="container">
        <h2 class="section-heading text-center">News</h2>
        <hr class="light">
                
        <!-- Projects Row -->
            <% if(typeof news !== 'undefined') { %>
        
        <div class="row">

                <% _.each(news, function(newsArticle){ %>
                        <div class="col-sm-4 news-item">
                            <div style="background-color:white;">
                                    <img class="img-responsive" src="<%= newsArticle.image %>" alt="<%= newsArticle.topic %>">
                                <div class="news-item--container">
                                    <h3 class="news-item--title"><%= newsArticle.topic %></h3>
                                    <p><%= newsArticle.description %></p>
                                    <small><%= newsArticle.createdAt %></small>
                                </div>
                            </div>
                        </div>
                <% }); %>
            
        </div>
        
        
            <% } %>
            
                
                
    </div>
</section>


